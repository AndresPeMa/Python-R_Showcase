    "    plt.plot(function)\n",
    "    plt.xlabel('Time $t$', fontsize=20)\n",
    "    plt.ylabel('$Y_t$', fontsize=20, rotation=0)\n",
    "    #plt.grid()\n",
    "    plt.show()"
   ]
